1.) Tribonacci Series :

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		    Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    int a = 0;
	    int b = 1;
	    int c = 1;
	    int next = 0;
	    System.out.println(a+" "+b+" "+c);
	    for(int i=4;i<n;i++)
	    
	    {
	        next = a+b+c;
	       a=b;
	       b=c;
	       c=next;
	       System.out.println(next);
	    }
	    
	}
}
	
